class Solution(object):
    def nextGreaterElements(self, nums):
        n = len(nums)
        st = []
        ans = [-1] * n
        for i in xrange(n * 2 - 1, -1, -1):
            while st and st[-1] <= nums[i % n]:
                st.pop()
            if i < n:
                ans[i] = st[-1] if st else -1
            st.append(nums[i % n])
        return ans
