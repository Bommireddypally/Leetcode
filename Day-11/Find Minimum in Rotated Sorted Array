#153. Find Minimum in Rotated Sorted Array
class Solution(object):
    def findMin(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        left, right = 0, len(nums) - 1

        if left == right:
            return nums[left]

        while left < right:
            mid = (left + right) // 2

            if right - left == 1:
                return min(nums[left], nums[right])

            if (nums[mid] < nums[right]):
                right = mid
            else:
                left = mid
